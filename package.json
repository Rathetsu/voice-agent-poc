{
  "name": "voice-agent-poc",
  "version": "1.0.0",
  "private": true,
  "description": "LiveKit Voice Agent Monorepo with Node.js backend and Next.js frontend",
  "scripts": {
    "dev": "pnpm --parallel --filter backend --filter frontend dev",
    "build": "pnpm --filter backend build && pnpm --filter frontend build",
    "backend:dev": "pnpm --filter backend dev",
    "backend:build": "pnpm --filter backend build",
    "backend:start": "pnpm --filter backend start",
    "backend:download-files": "pnpm --filter backend download-files",
    "frontend:dev": "pnpm --filter frontend dev",
    "frontend:build": "pnpm --filter frontend build",
    "frontend:start": "pnpm --filter frontend start"
  },
  "keywords": [
    "livekit",
    "voice-agent",
    "monorepo",
    "nextjs",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  }
}

